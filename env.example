# Configurações do Banco de Dados PostgreSQL
DATABASE_URL=postgres://postgres:postgres@localhost:5432/autoserp?sslmode=disable

# Configurações do Redis
REDIS_URL=redis://localhost:6379

# Chave secreta para JWT (IMPORTANTE: Altere em produção!)
JWT_SECRET=sua-chave-super-secreta-aqui

# Porta do servidor
PORT=3333

# Ambiente (development, production, test)
NODE_ENV=development

# Configuração do CORS - URLs permitidas (separadas por vírgula)
# Exemplos: http://localhost:3333,https://meusite.com,http://app.meudominio.com
ALLOWED_ORIGINS=http://localhost:3333,http://localhost:3001,http://localhost:8080 

# Senha do administrador master
MASTER_PASSWORD=senhaSuperSecreta 